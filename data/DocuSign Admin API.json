[
    {
        "user_name": "opatothan",
        "api_name": "DocuSign Admin API: 01 Authorize Code Grant Access Token",
        "api_call": "https://{{hostenv}}/oauth/token",
        "api_version": "NO VERSION IN POSTMAN",
        "api_arguments": {
            "method": "POST",
            "header": [
                {
                    "key": "Authorization",
                    "value": "Basic {{encodedKeys}}"
                }
            ],
            "body": {
                "mode": "urlencoded",
                "urlencoded": [
                    {
                        "key": "code",
                        "value": "{{codeFromUrl}}"
                    },
                    {
                        "key": "grant_type",
                        "value": "authorization_code"
                    }
                ]
            }
        },
        "functionality": "NO DESC IN POSTMAN"
    },
